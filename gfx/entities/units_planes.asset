# Internal scenes
	entity = {
		name = "attack_helicopter_equipment_1_entity"
		pdxmesh = "SOV_mi24_mesh"
		state = { name = "fire" animation = "attack" animation_speed = 3.0
			event = { time = 0 sound = { soundeffect = "helicopter_attack_animation" } }
			event = { trigger_once = yes sound = { soundeffect = "helicopter_attack_animation" } }
		}
		state = { name = "bomb" animation = "attack" animation_speed = 3.0
			event = { time = 0 sound = { soundeffect = "airplane_bomb" } }
			#event = { trigger_once = yes sound = { soundeffect = "distance_airplane_bomb" } }
		}
		state = { name = "idle" animation = "idle" animation_speed = 2.0
			event = { trigger_once = yes sound = { soundeffect = "helicopter_idle_animation" } }
			event = { trigger_once = yes sound = { soundeffect = "helicopter_idle_animation" } }
		}
		state = { name = "crash" animation = "move" animation_speed = 3.0
			event = { time = 0 sound = { soundeffect = "SOV_helicopter_crash_animation" } } 
			event = { trigger_once = yes sound = { soundeffect = "SOV_helicopter_crash_animation" } }
		}
		state = { name = "explode"  animation = "move" animation_speed = 3.0
			event = { time = 0 particle = "vehicle_explode_effect" keep_particle = yes }
			event = { trigger_once = yes sound = { soundeffect = "SOV_helicopter_crash_animation" } }
		}

	}

